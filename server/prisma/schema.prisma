generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}



model Animal {
  id       Int     @id @default(autoincrement())
  nome     String
  especie  String
  idade    Int
  vacinado Boolean
}

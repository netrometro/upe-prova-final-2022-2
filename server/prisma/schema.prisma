// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}

model GenshinWeapon {
  id            String  @id @default(cuid())
  name          String
  description   String @db.VarChar(1000)
  atk           Int
  weaponT5      Boolean
}

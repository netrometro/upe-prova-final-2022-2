generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}



model Animal {
  id       Int     @id @default(autoincrement())
  nome     String
  especie  String
  idade    Int
  vacinado Boolean
}


model DragQueen {
  id     Int       @id @default(autoincrement())
  name   String
  season Int
  winner Boolean
  info   String?
}

model Escola {
  id         Int    @id @default(autoincrement())
  name       String
  qntdSalas  Int
  qntdAlunos Int
  tipo       String
}


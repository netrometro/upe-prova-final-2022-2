// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}

model Music{
  id                Int      @id @default(autoincrement())
  nome              String
  artista           String
  album             String
  ano_lancamento    Int
  brasileira        Boolean
}

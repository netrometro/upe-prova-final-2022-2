// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}

model Cuphead_Enemy {
  id        String @id @default(cuid())
  type      String
  xPosition Float
  yPosition Float
  behavior  String
}

